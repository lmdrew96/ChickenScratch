{
  "name": "chicken-scratch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "seed": "tsx scripts/seed.ts",
    "route:check": "node scripts/check-routes.mjs",
    "route:manifest": "node scripts/route-manifest.mjs",
    "dev:crawl": "node scripts/dev-crawl.mjs",
    "prod:crawl": "node scripts/prod-crawl.mjs",
    "verify:dev": "pnpm route:manifest && pnpm dev:crawl",
    "verify:prod": "pnpm route:manifest && pnpm prod:crawl",
    "verify:all": "pnpm typecheck && pnpm lint && pnpm verify:dev && pnpm verify:prod"
  },
  "dependencies": {
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.57.4",
    "clsx": "2.1.1",
    "next": "15.5.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@playwright/test": "^1.55.0",
    "@types/node": "20.19.17",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "autoprefixer": "10.4.21",
    "eslint": "9.11.1",
    "eslint-config-next": "15.5.3",
    "postcss": "8.5.6",
    "tailwindcss": "3.4.17",
    "tsx": "4.20.5",
    "typescript": "5.6.3",
    "vitest": "2.1.9"
  }
}
